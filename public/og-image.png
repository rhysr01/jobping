<svg width="1200" height="630" viewBox="0 0 1200 630" xmlns="http://www.w3.org/2000/svg">
  <defs>
    <linearGradient id="bgGradient" x1="0%" y1="0%" x2="100%" y2="100%">
      <stop offset="0%" style="stop-color:#0B0B0F;stop-opacity:1" />
      <stop offset="100%" style="stop-color:#151519;stop-opacity:1" />
    </linearGradient>
    <linearGradient id="accentGradient" x1="0%" y1="0%" x2="100%" y2="100%">
      <stop offset="0%" style="stop-color:#3B82F6;stop-opacity:1" />
      <stop offset="100%" style="stop-color:#1D4ED8;stop-opacity:1" />
    </linearGradient>
  </defs>
  
  <!-- Background -->
  <rect width="1200" height="630" fill="url(#bgGradient)"/>
  
  <!-- Decorative elements -->
  <circle cx="100" cy="100" r="50" fill="url(#accentGradient)" opacity="0.1"/>
  <circle cx="1100" cy="530" r="80" fill="url(#accentGradient)" opacity="0.1"/>
  <circle cx="200" cy="500" r="30" fill="url(#accentGradient)" opacity="0.1"/>
  
  <!-- Main content area -->
  <g transform="translate(100, 150)">
    <!-- JobPing Logo -->
    <text x="0" y="80" font-family="Inter, -apple-system, BlinkMacSystemFont, sans-serif" font-size="72" font-weight="400" fill="#FFFFFF" letter-spacing="-0.02em">
      <tspan font-weight="400">Job</tspan><tspan font-weight="600">Ping</tspan>
    </text>
    
    <!-- Tagline -->
    <text x="0" y="140" font-family="Inter, -apple-system, BlinkMacSystemFont, sans-serif" font-size="32" font-weight="400" fill="#9CA3AF" letter-spacing="-0.01em">
      AI-powered job matching for students
    </text>
    
    <!-- CTA -->
    <rect x="0" y="200" width="300" height="60" rx="12" fill="#FFFFFF"/>
    <text x="150" y="240" font-family="Inter, -apple-system, BlinkMacSystemFont, sans-serif" font-size="20" font-weight="600" fill="#0B0B0F" text-anchor="middle">
      Get matched daily
    </text>
  </g>
  
  <!-- Visual elements -->
  <g transform="translate(800, 200)">
    <!-- Email icon -->
    <rect x="0" y="0" width="80" height="60" rx="8" fill="url(#accentGradient)" opacity="0.2"/>
    <path d="M20 20 L40 35 L60 20" stroke="#3B82F6" stroke-width="3" fill="none"/>
    <rect x="15" y="15" width="50" height="30" rx="4" stroke="#3B82F6" stroke-width="2" fill="none"/>
    
    <!-- Job cards -->
    <rect x="100" y="20" width="120" height="40" rx="6" fill="#374151" opacity="0.8"/>
    <rect x="110" y="30" width="100" height="4" rx="2" fill="#9CA3AF"/>
    <rect x="110" y="40" width="60" height="4" rx="2" fill="#6B7280"/>
    
    <rect x="100" y="70" width="120" height="40" rx="6" fill="#374151" opacity="0.8"/>
    <rect x="110" y="80" width="90" height="4" rx="2" fill="#9CA3AF"/>
    <rect x="110" y="90" width="70" height="4" rx="2" fill="#6B7280"/>
  </g>
  
  <!-- Bottom accent -->
  <rect x="0" y="580" width="1200" height="50" fill="url(#accentGradient)" opacity="0.1"/>
</svg>
